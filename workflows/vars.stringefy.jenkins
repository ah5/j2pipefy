#!/usr/bin/env groovy
import groovy.transform.Field
import java.lang.Exception

@Field def script

def getScript() {
    if(script == null) {
        script = load 'vars/stringefy.groovy'
    }
    return script
}

def isEmpty() {
   node() {
       getScript().isEmpty("")
   }
}

def isNotEmpty() {
   node() {
       getScript().isNotEmpty "message"
   }
}

return this